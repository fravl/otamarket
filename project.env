# Database configuration for PostgreSQL (running in container called "ota_db_cont")
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_DB=database

# Database configuration for Flyway (used for database migrations)
FLYWAY_USER=username
FLYWAY_PASSWORD=password
FLYWAY_URL=jdbc:postgresql://ota_db_cont:5432/database

DATABASE_URL=postgres://username:password@ota_db_cont:5432/database